<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1500">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="155dp"
            android:layout_marginTop="216dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="138dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="108dp"
            android:layout_marginStart="108dp"
            android:id="@+id/imLogo"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="108dp"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="372dp"
            android:id="@+id/tvName"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_marginEnd="8dp"
            android:layout_height="275dp"
            android:layout_marginTop="396dp"
            android:layout_marginRight="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="135dp"
            android:id="@+id/imTwoTree" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="580dp"
            android:visibility="invisible"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="108dp"
            android:id="@+id/main_holder" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="invisible"
            android:layout_marginEnd="0dp"
            android:id="@+id/bSettings" />
        <Constraint
            motion:layout_constraintEnd_toStartOf="@+id/bSettings"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="invisible"
            android:layout_marginEnd="0dp"
            android:id="@+id/bCalc" />
        <Constraint
            motion:layout_constraintStart_toEndOf="@+id/bHome"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="invisible"
            android:layout_marginStart="0dp"
            android:id="@+id/bHistory" />
        <Constraint
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="invisible"
            android:layout_marginStart="0dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/bHome" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="111dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="108dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="84dp"
            android:layout_marginStart="84dp"
            android:id="@+id/imLogo" />
        <Constraint
            android:layout_marginEnd="68dp"
            android:layout_height="57dp"
            android:layout_marginTop="32dp"
            android:layout_marginRight="68dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="97dp"
            android:id="@+id/tvName" />
        <Constraint
            android:layout_height="110dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="83dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:id="@+id/ivThreeTree" />
        <Constraint
            android:layout_height="106dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="79dp"
            android:id="@+id/imTwoTree"
            motion:layout_editor_absoluteX="277dp" />
        <Constraint
            android:layout_width="83dp"
            android:layout_height="110dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="0dp"
            android:id="@+id/ivThreeTree" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="79dp"
            android:layout_height="106dp"
            android:layout_marginEnd="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp"
            android:id="@+id/imTwoTree"
            motion:layout_editor_absoluteX="277dp" />
        <Constraint
            android:id="@+id/bHome"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            android:layout_marginStart="0dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/bHistory"
            motion:layout_constraintStart_toEndOf="@+id/bHome"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            android:layout_marginStart="0dp" />
        <Constraint
            android:id="@+id/bCalc"
            motion:layout_constraintEnd_toStartOf="@+id/bSettings"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            android:layout_marginEnd="0dp" />
        <Constraint
            android:id="@+id/bSettings"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            android:layout_marginEnd="0dp" />
        <Constraint
            android:id="@+id/main_holder"
            android:layout_width="match_parent"
            android:layout_height="580dp"
            android:visibility="visible"
            android:layout_marginStart="0dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="108dp"
            android:layout_marginLeft="4dp" />
    </ConstraintSet>
</MotionScene>