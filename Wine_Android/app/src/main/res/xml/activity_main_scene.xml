<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1500">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="111dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="108dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="96dp"
            android:layout_marginStart="96dp"
            android:id="@+id/imLogo" />
        <Constraint
            android:layout_marginEnd="88dp"
            android:layout_height="57dp"
            android:layout_marginTop="28dp"
            android:layout_marginRight="88dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="97dp"
            android:id="@+id/tvName" />
        <Constraint
            android:layout_height="138dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="91dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:id="@+id/ivThreeTree" />
        <Constraint
            android:layout_height="106dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="79dp"
            android:id="@+id/imTwoTree"
            motion:layout_editor_absoluteX="277dp" />
        <Constraint
            android:layout_width="91dp"
            android:layout_height="138dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="0dp"
            android:id="@+id/ivThreeTree" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="79dp"
            android:layout_height="106dp"
            android:layout_marginEnd="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp"
            android:id="@+id/imTwoTree"
            motion:layout_editor_absoluteX="277dp" />
        <Constraint
            android:id="@+id/bHome"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            android:layout_marginStart="8dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/bHistory"
            motion:layout_constraintStart_toEndOf="@+id/bHome"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            android:layout_marginStart="6dp" />
        <Constraint
            android:id="@+id/bCalc"
            motion:layout_constraintEnd_toStartOf="@+id/bSettings"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            android:layout_marginEnd="6dp" />
        <Constraint
            android:id="@+id/bSettings"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="90dp"
            android:layout_height="90dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            android:layout_marginEnd="8dp" />
        <Constraint
            android:id="@+id/main_holder"
            android:layout_width="match_parent"
            android:layout_height="465dp"
            android:visibility="visible"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="168dp" />
    </ConstraintSet>
</MotionScene>